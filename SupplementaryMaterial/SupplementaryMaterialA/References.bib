	pages = {197--211},
	file = {Chiari et al. - 2005 - Human movement analysis using stereophotogrammetry.pdf:/home/andy-pohl/Zotero/storage/FE5Q7LV7/Chiari et al. - 2005 - Human movement analysis using stereophotogrammetry.pdf:application/pdf;Chiari et al. - 2005 - Human movement analysis using stereophotogrammetry.pdf:/home/andy-pohl/Zotero/storage/FYI4Z5ZP/Chiari et al. - 2005 - Human movement analysis using stereophotogrammetry.pdf:application/pdf}
